{
	"name": "df_fix_csv_to_parquet_week",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_export_week_csv",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "fct_export_week_parquet_sink",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     p_outFolder as string (@replace(item().name, '.csv', '.parquet')),",
				"     p_outFile as string (@concat(pipeline().parameters.p_outRoot,'/', items('FE_Years').name)),",
				"     p_inFolder as string (@concat(pipeline().parameters.p_inRoot,'/', items('FE_Years').name)),",
				"     p_inFile as string (@item().name)",
				"}",
				"source(output(",
				"          Systeemhuis as string,",
				"          EventNaam as string,",
				"          KantoorId as string,",
				"          MaatschappijId as string,",
				"          Verzekeraar as string,",
				"          Jaar as string,",
				"          Week as string,",
				"          Message as string,",
				"          TimeStamp as string,",
				"          GebruikerId as string,",
				"          FunctieId as string,",
				"          Productomschrijving as string,",
				"          FunctieSoort as string,",
				"          FunctieType as string,",
				"          KantoorGebruikerId as string,",
				"          User_identity as string,",
				"          role_group as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive(FunctieId = iif( isNull(FunctieId), null(),replace(trim(toString(FunctieId)), ',', '.') )) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     partitionFileNames:['$p_outFile'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}