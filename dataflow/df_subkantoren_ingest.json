{
	"name": "df_subkantoren_ingest",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ref_subkantoor",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ref_subkantoor_actief",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          kantoorId as string,",
				"          GebruikerId as string,",
				"          {Naam subkantoor} as string,",
				"          Begindatum as string,",
				"          Einddatum as string,",
				"          {Einddatum is t/m en geldt voor hele kantoor niet voor subkantoor (die wordt met * aangegeven)} as string,",
				"          {Gecontroleerd op dubbele 14-2-2024} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 filter(toDate(Einddatum) >= currentDate()) ~> filter1",
				"select1 derive(KantoorGebruikerId = replace(     toString(KantoorGebruikerId), '-',     '_' )) ~> derivedColumn1",
				"filter1 select(mapColumn(",
				"          Subkantoor = {Naam subkantoor},",
				"          KantoorGebruikerId = {Einddatum is t/m en geldt voor hele kantoor niet voor subkantoor (die wordt met * aangegeven)},",
				"          Begindatum,",
				"          Einddatum",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['subkantoor.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}